{
	"openapi": "3.0.0",
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"UserProfile": {
				"properties": {
					"id": {
						"type": "number",
						"format": "double",
						"example": 1
					},
					"uuid": {
						"type": "string",
						"example": "123e4567-e89b-12d3-a456-426614174000"
					},
					"username": {
						"type": "string",
						"example": "admin"
					},
					"email": {
						"type": "string",
						"example": "admin@example.com"
					},
					"displayName": {
						"type": "string",
						"nullable": true,
						"example": "John Doe"
					},
					"role": {
						"type": "string",
						"enum": [
							"USER",
							"MOD",
							"ADMIN"
						],
						"example": "USER"
					}
				},
				"required": [
					"id",
					"uuid",
					"username",
					"email",
					"role"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Role": {
				"type": "string",
				"enum": [
					"USER",
					"MOD",
					"ADMIN"
				]
			},
			"RegisterRequest": {
				"properties": {
					"username": {
						"type": "string",
						"example": "newuser"
					},
					"password": {
						"type": "string",
						"example": "secret123"
					},
					"email": {
						"type": "string",
						"example": "user@example.com"
					},
					"displayName": {
						"type": "string",
						"nullable": true,
						"example": "John Doe"
					}
				},
				"required": [
					"username",
					"password",
					"email"
				],
				"type": "object",
				"additionalProperties": false
			},
			"AuthResponse": {
				"properties": {
					"token": {
						"type": "string",
						"example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
					}
				},
				"required": [
					"token"
				],
				"type": "object",
				"additionalProperties": false
			},
			"LoginRequest": {
				"properties": {
					"username": {
						"type": "string",
						"example": "admin"
					},
					"password": {
						"type": "string",
						"example": "admin"
					}
				},
				"required": [
					"username",
					"password"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {
			"jwt": {
				"type": "apiKey",
				"name": "Authorization",
				"in": "header"
			}
		}
	},
	"info": {
		"title": "insane-node-rest-api",
		"version": "1.0.0",
		"license": {
			"name": "MIT"
		},
		"contact": {
			"name": "lucasdee"
		}
	},
	"paths": {
		"/users/me": {
			"get": {
				"operationId": "GetProfile",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UserProfile"
								}
							}
						}
					}
				},
				"tags": [
					"Users"
				],
				"security": [
					{
						"jwt": []
					}
				],
				"parameters": []
			}
		},
		"/auth/register": {
			"post": {
				"operationId": "Register",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"createdAt": {
											"type": "string",
											"format": "date-time"
										},
										"role": {
											"$ref": "#/components/schemas/Role"
										},
										"displayName": {
											"type": "string"
										},
										"email": {
											"type": "string"
										},
										"password": {
											"type": "string"
										},
										"username": {
											"type": "string"
										},
										"uuid": {
											"type": "string"
										},
										"id": {
											"type": "number",
											"format": "double"
										}
									},
									"required": [
										"createdAt",
										"role",
										"displayName",
										"email",
										"password",
										"username",
										"uuid",
										"id"
									],
									"type": "object"
								}
							}
						}
					}
				},
				"tags": [
					"Auth"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/RegisterRequest"
							}
						}
					}
				}
			}
		},
		"/auth/login": {
			"post": {
				"operationId": "Login",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/AuthResponse"
								}
							}
						}
					}
				},
				"tags": [
					"Auth"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/LoginRequest"
							}
						}
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "/"
		}
	]
}